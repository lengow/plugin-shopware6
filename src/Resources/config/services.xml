<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="Lengow\Connector\Entity\Lengow\Action\ActionDefinition">
            <tag name="shopware.entity.definition" entity="lengow_action" />
        </service>

        <service id="Lengow\Connector\Entity\Lengow\Order\OrderDefinition">
            <tag name="shopware.entity.definition" entity="lengow_order" />
        </service>

        <service id="Lengow\Connector\Entity\Lengow\OrderError\OrderErrorDefinition">
            <tag name="shopware.entity.definition" entity="lengow_order_error" />
        </service>

        <service id="Lengow\Connector\Entity\Lengow\OrderLine\OrderLineDefinition">
            <tag name="shopware.entity.definition" entity="lengow_order_line" />
        </service>

        <service id="Lengow\Connector\Entity\Lengow\Product\ProductDefinition">
            <tag name="shopware.entity.definition" entity="lengow_product" />
        </service>

        <service id="Lengow\Connector\Entity\Lengow\Settings\SettingsDefinition">
            <tag name="shopware.entity.definition" entity="lengow_settings" />
        </service>

    </services>
</container>
