                        <div class="lgw-row">
                            <div class="lgw-box lgw-home-header text-center">
                                <img :src="'connector/lengow-white-big.png' | asset" alt="lengow" class="img-responsive">
                                <h1 style="color: white">{{ $tc('lengow-connector.dashboard.banner.title') }}</h1>
                                <sw-button :disabled="false" :square="false" :block="false" :isLoading="false" style="margin-top: 15px" class="lgw-btn">
